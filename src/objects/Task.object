    <validationRules>
        <fullName>Finish_all_call_monitor_field</fullName>
        <active>true</active>
        <errorConditionFormula>AND(!ISBLANK(Monitored_By__c),
    OR(ISBLANK( TEXT(CM_advised_what_will_happen_next__c)),
       ISBLANK( TEXT(Allow_customer_to_present_concerns__c) ),
       ISBLANK( TEXT(Communication_is_clear_positive_confiden__c) ),
       ISBLANK( TEXT(Demonstrate_active_listening__c )),
       ISBLANK( TEXT(Gave_correct_answers_and_infomations__c) ),
       ISBLANK( TEXT(Mirrors_clients_talking_behaviour__c) ) ))</errorConditionFormula>
        <errorMessage>Please complete all call monitoring checklist</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NVMContactWorld__Interac_Quality_Must_Be_Between_1_and_10</fullName>
        <active>true</active>
        <errorConditionFormula>NVMContactWorld__InteractionQuality__c &lt; 1  ||  NVMContactWorld__InteractionQuality__c &gt; 10</errorConditionFormula>
        <errorMessage>Interaction Quality must be a number between 1 and 10</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>activities__CloseTasks</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Close Tasks</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/38.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/38.0/apex.js&quot;)}
var left = (screen.width/2)-(320/2);
var top = (screen.height/2)-(200/2);
var records = {!GETRECORDIDS($ObjectType.Task)};
var strIDs=&apos;&apos;;
if (records[0] == null)
{
alert(&apos;Please select atleast one record&apos;);
}
else
{
for (var n=0; n&lt;records.length; n++)
{
if(strIDs == &apos;&apos;)
strIDs = records[n];
else
strIDs = strIDs + &quot;,&quot; + records[n];
}
window.location.href = &apos;/apex/activities__CloseTasks?strIDs=&apos;+strIDs+&apos;&amp;retURL=&apos;+ window.location.href;
}</url>
    </webLinks>
    <webLinks>
        <fullName>activities__Mass_Update</fullName>
