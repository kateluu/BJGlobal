    <validationRules>
        <fullName>Action_Taken_to_Save</fullName>
        <active>false</active>
        <description>Check if the Action_taken_to_save field is blank or same as help text.</description>
        <errorConditionFormula>AND (INCLUDES( CaseType__c , &apos;Cancellation&apos;), OR(ISBLANK( X2012_Action_Taken_to_Save__c), X2012_Action_Taken_to_Save__c = &apos;If you have attempted to save the client, please detail your save strategy.&apos;))</errorConditionFormula>
        <errorDisplayField>X2012_Action_Taken_to_Save__c</errorDisplayField>
        <errorMessage>Action_taken_to_save cannot be blank or same as help text if case type is Cancellation</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Billing_Resolution_Credit</fullName>
        <active>false</active>
        <description>Billing Resolution Credit</description>
        <errorConditionFormula>ISPICKVAL(Billing_Resolution__c, &apos;Credit&apos;)  &amp;&amp;  ISBLANK( Credit_Amount__c )</errorConditionFormula>
        <errorDisplayField>Credit_Amount__c</errorDisplayField>
        <errorMessage>Please enter a credit amount</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Billing_Resolution_Refund</fullName>
        <active>false</active>
        <description>Billing Resolution Refund</description>
        <errorConditionFormula>ISPICKVAL(Billing_Resolution__c, &apos;Refund&apos;)  &amp;&amp;  ISBLANK( Refund_Amount__c )</errorConditionFormula>
        <errorDisplayField>Refund_Amount__c</errorDisplayField>
        <errorMessage>Please enter a refund amount</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Have_Confirmed_When_Unresolved</fullName>
        <active>true</active>
        <errorConditionFormula>AND( NOT( 
INCLUDES(CaseType__c, &apos;Upgrade&apos;)|| 
INCLUDES(CaseType__c, &apos;Downgrade&apos;) 
), 
AND(ISPICKVAL(Status__c , &apos;Unresolved&apos;),
AND(ISBLANK(If_no_please_explain__c), Have_Confirmed_to_Turn_Off_AdWords_Acct__c =false),
NOT(CONTAINS(  Owner_Role__c , &quot;SEO&quot;))
))</errorConditionFormula>
        <errorDisplayField>Have_Confirmed_to_Turn_Off_AdWords_Acct__c</errorDisplayField>
        <errorMessage>Have you confirmed if client would like Adwords Account turned off, if no, please explain</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Client_Manager_Reassign_To</fullName>
        <active>true</active>
        <description>Client Manager Reassign To</description>
        <errorConditionFormula>Client_Manager_Reassign__c = TRUE &amp;&amp; ISBLANK( Reassign_To__c )</errorConditionFormula>
        <errorDisplayField>Reassign_To__c</errorDisplayField>
        <errorMessage>Please select a user account to reassign to</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closed_Cases</fullName>
        <active>true</active>
        <description>Only Accounts and COM can change Final Action (FOR MANAGERS) and Case Resolution once case being Closed</description>
        <errorConditionFormula>AND( 
OR( 
ISCHANGED(Case_Resolution__c), 
ISCHANGED(X2012_Final_Action_FOR_MANAGERS__c)
), 
ISPICKVAL(Status__c, &quot;Closed&quot;), 
$Profile.Name &lt;&gt; &quot;Accounts &amp; Admin&quot;,
$Profile.Name &lt;&gt; &quot;System Administrator&quot;,
$Profile.Name &lt;&gt; &quot;SLX Client Operations Manager&quot; 
)</errorConditionFormula>
        <errorMessage>Sorry, you can&apos;t alter Case Resolution or Final Action (FOR MANAGERS) once Case being closed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>For_Unresolved_Approval_Require</fullName>
        <active>true</active>
        <description>Approval is Required for any Unresolved Request</description>
        <errorConditionFormula>OR(
AND(
AND(ISPICKVAL(PRIORVALUE(Status__c),&quot;Escalated&quot;),
OR((Downgrade_Difference__c&gt;= 10),
(Refund_Amount__c  &gt; 500))), (ISPICKVAL(Status__c,&quot;Unresolved&quot;)),
 $User.Username &lt;&gt; &apos;amber.porter@sponsoredlinx.com&apos;
)
)</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Approval is required to become &apos;Unresolved&apos; status because the Downgrade Amount or Refund Request Amount is too big</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Client_Manager_to_edit_record</fullName>
        <active>true</active>
        <description>Prevent_Client_Manager_to_edit_record</description>
        <errorConditionFormula>ISPICKVAL(Status__c , &apos;Escalated&apos;) 
&amp;&amp; INCLUDES(CaseType__c, &apos;Cancellation&apos;) 
&amp;&amp; ( 
CONTAINS(Product_Type__c, &apos;AdWords&apos;) || 
CONTAINS(Product_Type__c, &apos;Social&apos;) 
) 
&amp;&amp; (OwnerId = $User.Id)
&amp;&amp; (NOT(ISCHANGED(Status__c)))</errorConditionFormula>
        <errorMessage>Sorry. It is an escalated case , please contact the Resolution Manager to request any update. Thanks</errorMessage>
    </validationRules>
