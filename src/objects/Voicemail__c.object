    <fields>
        <fullName>Business_Processing_Hour__c</fullName>
        <description>The business hour difference between the call response time &amp; created time</description>
        <externalId>false</externalId>
        <formula>ROUND(8*( 
(5*FLOOR((DATEVALUE(Response_Time__c)-DATE(1996,01,01))/7) + 
MIN(5, 
MOD(DATEVALUE(Response_Time__c)-DATE(1996,01,01), 7) + 
MIN(1, 24/8*(MOD(Response_Time__c -DATETIMEVALUE(&apos;1996-01-01 22:00:00&apos;), 1))) 
)) 
- 
(5*FLOOR((DATEVALUE(CreatedDate)-DATE(1996,01,01))/7) + 
MIN(5, 
MOD(DATEVALUE(CreatedDate)-DATE(1996,01,01), 7) + 
MIN(1, 24/8*(MOD(CreatedDate-DATETIMEVALUE(&apos;1996-01-01 22:00:00&apos;), 1))) 
)) 
), 2)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>The business hour difference between the call response time &amp; created time</inlineHelpText>
        <label>Business Processing Hour</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
