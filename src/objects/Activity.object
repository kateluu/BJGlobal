    <fields>
        <fullName>Email_Response_Time__c</fullName>
        <externalId>false</externalId>
        <formula>ROUND(8*(
(5*FLOOR((DATEVALUE(CreatedDate)-DATE(1996,01,01))/7) +
MIN(5, 
    MOD(DATEVALUE(CreatedDate)-DATE(1996,01,01), 7) +
    MIN(1, 24/8*(MOD(CreatedDate -DATETIMEVALUE(&apos;1996-01-01 22:00:00&apos;), 1)))
))
-
(5*FLOOR((DATEVALUE(Email_Received_Date_Business_Hour__c)-DATE(1996,01,01))/7) +
MIN(5, 
    MOD(DATEVALUE(Email_Received_Date_Business_Hour__c)-DATE(1996,01,01), 7) +
    MIN(1, 24/8*(MOD(Email_Received_Date_Business_Hour__c-DATETIMEVALUE(&apos;1996-01-01 22:00:00&apos;), 1)))
))
), 2)</formula>
        <label>Email Response Time</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
